<div class="col-md-4 btn-calandar-description-alignment">
  <div class="wrap">
      <table class="head">
      <tbody><tr>
      <div>
        <th style="background-color: #003366;color: #fff;">
          <span class="text">
          <% @date = @date.blank? ? Date.today.strftime("%d %B %Y") : @date %>
            <center><%= @date %></center>
          </span>
        </th>
      </div>
      </tr>
     </tbody>
     </table>
     <div class="inner_table calendar-entry-table-height">
          <table>
          <tbody>
          <% if @events.count == 0 %>
            <tr>No event scheduled for this date</tr>
          <% end %>
      <% @events.each do |event| %>
      <tr>
        <td><div class="col-md-10 heading-description-calendar"> <%= link_to event.name.titleize, all_images_photographer_client_event_path(current_photographer, event.client, event) %></div>
        <div class="col-md-2 btn-calandar-description-alignment">
        <button type="button" class="btn-edit-calendar" ><i class="fa  fa-edit btn-edit btn-style"></i></button>
        <button type="button" class="btn-edit-calendar"><i class="fa  fa-times btn-del btn-style" ></i></button>
        </div>
        <div class="col-md-12 detail-description-calendar" style="">
          <% if event.groom.present? %>
            <%= event.groom.titleize %> and <%= event.bridal.titleize %> <br>
          <% end %>
          <%= event.location %><br>
          <%= event.start_time.strftime('%a %b %d %Y %H:%M') %>
        </div>
        </td>
      </tr>
      <% end %>
    </tbody></table></div></div>
    <button class="button button4 popup event-btn" id="popup" data-id="abc2"><i class="fa fa-plus-circle"></i> Calender Entry </button>
    <%= image_tag ("loader.gif"), id: "loading-image", style: "display:none; position: absolute; top: 208px;" %>
</div>