<div class="center-align-icons">
  <div class="container-fluid">
    <div class="col-md-12">
      <div class="col-md-3">
        <center>
        <%= link_to photographer_clients_path(current_photographer) do %>
        <div class="boxhead pictureContainer">
          <%= image_tag("client-management-icon.svg") %>
        </div>
        <p class="menu-first-page">Client Management</p>
        <% end %>
        </center>
      </div>
      <div class="col-md-3">
        <center>
        <%= link_to photographer_events_info_path(current_photographer) do %>
          <div class="boxhead pictureContainer">
            <%= image_tag 'my-calendar-icon.svg'%>
          </div>
          <p class="menu-first-page">My Calendar</p>
        <% end %>
        </center>
      </div>
      <div class="col-md-3">
        <center>
        <%= link_to photographer_albums_path(current_photographer) do %>
        <div class="boxhead pictureContainer">  
            <%= image_tag("album-icon.svg") %>
          </div>
          <p class="menu-first-page">My Albums</p>
        <% end %>
        </center>
      </div>
      <div class="col-md-3">
        <center>
        <%= link_to settings_photographer_path(current_photographer) do %>
        <div class="boxhead pictureContainer">
          <%= image_tag("connect-icon.svg") %>
        </div>
        <p class="menu-first-page">Connects</p>
        <% end %>
        </center>
      </div>
    </div>
  </div>
