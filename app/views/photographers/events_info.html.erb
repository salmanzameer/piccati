<div class="dropdown col-md-12 drop-down-btn-cal">
  <button class="btn btn-default dropdown-toggle button_class drop-down-btn-cal-beauty" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
    Monthly
    <span class="caret drop-down-btn-cal-beauty"></span>
  </button>
  <ul class="dropdown-menu dropdown-cal-ul" aria-labelledby="dropdownMenu1">
    <li class="btn-drop-padding-li"><%= link_to "Monthly", photographer_events_info_path(calendar_type: "month"), class: 'month-sel link-padding-zero', remote: true %></li>
    <li><%= link_to "Weekly", photographer_events_info_path(calendar_type: "week"), class: 'week-sel link-padding-zero', remote: true %></li>
  </ul>
</div>
<div class="col-md-8 calendar-box">
	<% if params[:calendar_type] == "week" %>
		<%= render partial: 'week_calendar', locals: {events: @calendar_event} %>
	<% else %>
		<%= render partial: 'month_calendar', locals: {events: @calendar_event} %>
	<% end %>
</div>
<div class="scheduled-events">
  <%= render partial: "events/secheduled_events" %>
</div>
		<div id="abc2">
		<!-- Popup Div Starts Here -->
			<div id="popupContact">
	<!-- Contact Us Form -->
				<%= form_for Event.new, url: photographer_client_events_path(current_photographer.id, Client.first), html: { class: "form-action" } do |f| %>
					<%= image_tag ("close1.png"), class: "close", data: { id: "abc2" }%>
					<center><h2>Add a Event</h2></center>
					<fieldset>
					  <%= f.text_field :name, autofocus: true, :placeholder =>"Name", :class => "first-field client_fields" %>
					  <%= f.text_field :location, autofocus: true, :placeholder =>"Location", :class => "first-field client_fields" %>
					  <%= f.text_field :bridal, autofocus: true, :placeholder =>"Bride Name", :class => "first-field client_fields" %>
					  <%= f.text_field :groom, autofocus: true, :placeholder =>"Groom Name", :class => "first-field client_fields" %>
					  <div class='input-group date' id='datetimepicker1'>
                    <input type='datetime' class="first-field client_fields" name="event[start_time]" placeholder="Select Date" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            <script type="text/javascript">
		            $(function () {
		                $('#datetimepicker1').datetimepicker();
		            });
		        </script>
					 </fieldset>
					<!-- <textarea id="msg" name="message" placeholder="Message"></textarea> -->
					
					<center><%= f.button 'Add Event' , id: "submit", class: "button4 submit-event" %></center>
				<% end %>
			</div>
	<!-- Popup Div Ends Here -->
		</div>