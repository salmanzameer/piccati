<% if current_photographer.present? %>
<nav class="navbar navbar-default navbar-fixed-top">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#"><%= link_to image_tag("/images.png", :width => 80, :height => 54,  :crop => :thumb, :gravity => :face, :radius => :max , :align => 'left'),api_v1_photographer_path(current_photographer)%></a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
       
        <li><%= link_to('Clients', api_v1_photographer_clients_path(current_photographer)) if current_photographer.present?%></li>
        <li><%= link_to('Events', api_v1_photographer_eventsinfo_path(current_photographer)) if current_photographer.present? %></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
       
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Account <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li><%= link_to('Profile',api_v1_photographer_path(current_photographer)) if current_photographer.present? %></li><br>
            <li><%= link_to('Edit', edit_photographer_registration_path) if current_photographer.present? %> </li><br>
          </ul>
          <li><%= link_to('logout', destroy_photographer_session_path,:method => :delete) if current_photographer.present? %></li>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>
<% end %>