<h1>Editing Album</h1>

<%= form_for @album, url: photographer_album_path do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

