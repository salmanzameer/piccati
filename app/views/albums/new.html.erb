<h1>New Album</h1>
<br><br><br><br><br>
<%= form_for @album, url: photographer_albums_path do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Back', photographer_albums_path %>
